  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "types": [
      "puppeteer"
    ],
    "outDir": "dist",
    "resolveJsonModule": true
  }
}